{
  "name": "node-red-contrib-solaredge-optimizers",
  "version": "0.4.0",
  "description": "Read optimizer data from solaredge cloud",
  "main": "dist/nodes/solaredge-optimizers.js",
  "scripts": {
    "build": "tsc && copyfiles -u 2 src/nodes/*.html dist/nodes",
    "build:watch": "tsc --watch",
    "dev": "npm run build:watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "node-red": "node /usr/local/lib/node_modules/node-red/red.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "node-red": {
    "version": ">=3.1.1",
    "nodes": {
      "solaredge-optimizers": "dist/nodes/solaredge-optimizers.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "node-red",
    "solaredge",
    "optimizers",
    "typescript"
  ],
  "author": "Daniel Buchberger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/danisahni/node-red-contrib-solaredge-optimizers"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "axios-cookiejar-support": "^5.0.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@types/node-red": "^1.3.5",
    "@types/tough-cookie": "^4.0.5",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.3"
  }
}
